            android:name="com.guru.coursework.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".registerActivity" >
        </activity>
        <activity android:name=".ATContainer" >
        </activity>
        <activity android:name=".DetailsActivity" >
        </activity>
        <activity android:name=".CourseProgress" >
        </activity>
        <activity android:name=".Messages" >
        </activity>
        <activity android:name=".ContactDetails" >
        </activity>
    </application>

</manifest>